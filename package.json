{
	"name": "nervo",
	"version": "0.0.5",
	"description": "[WIP] Javascript Animation Library",
	"main": "build/nervo.min.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SlimMarten/nervo.git"
	},
	"scripts": {
		"build:prod": "NODE_ENV=production webpack --config config/webpack.config.js --mode production",
		"build:dev": "NODE_ENV=development webpack --config config/webpack.config.js --mode development",
		"build:app": "NODE_ENV=production webpack --config config/webpack.config.app.js --mode production",
		"build": "npm-run-all build:prod build:dev build:app",
		"app": "NODE_ENV=development webpack-dev-server --config config/webpack.config.app.js --mode development --open",
		"start": "NODE_ENV=development webpack-dev-server --config config/webpack.config.js --hot --mode development --open"
	},
	"keywords": [
		"javascript",
		"tween",
		"animation",
		"motion",
		"js",
		"timeline"
	],
	"author": "Marten Zander",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/plugin-proposal-class-properties": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"@babel/preset-react": "^7.0.0",
		"autoprefixer": "^9.5.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"babel-plugin-prismjs": "^1.0.2",
		"babel-preset-react": "^6.24.1",
		"core-js": "^3.0.0",
		"css-loader": "^2.1.1",
		"eslint": "^5.15.3",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-babel": "^5.3.0",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.5.0",
		"npm-run-all": "^4.1.5",
		"postcss-import": "^12.0.1",
		"postcss-loader": "^3.0.0",
		"postcss-mesh": "^1.1.2",
		"postcss-sassy-mixins": "^2.1.0",
		"postcss-scss": "^2.0.0",
		"prismjs": "^1.16.0",
		"raw-loader": "^2.0.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"style-loader": "^0.23.1",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.3.0",
		"webpack-dev-server": "^3.2.1"
	},
	"dependencies": {
		"@babel/polyfill": "^7.2.5",
		"eases": "^1.0.8"
	}
}
