{
	"name": "zeitgeist-js",
	"version": "0.0.1",
	"description": "Javascript Animation Library",
	"main": "build/zeitgeist-js.min.js",
	"scripts": {
		"build:prod": "NODE_ENV=production webpack --config config/webpack.config.js --mode production",
		"build:dev": "NODE_ENV=development webpack --config config/webpack.config.js --mode development",
		"build": "npm-run-all build:prod build:dev",
		"code": "NODE_ENV=development webpack-dev-server --config config/webpack.config.js --hot --inline --mode development --open",
		"style": "postcss demo/css/src/styles.css --watch --dir demo/css/ --config config/postcss.config.js ",
		"start": "npm-run-all --parallel style code"
	},
	"keywords": ["javascript", "tween", "animation", "motion", "js", "timeline"],
	"author": "Marten Zander",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.4",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"css-loader": "^2.1.0",
		"eslint": "^5.13.0",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-babel": "^5.3.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^7.0.14",
		"postcss-advanced-variables": "^3.0.0",
		"postcss-at-rules-variables": "^0.1.7",
		"postcss-calc": "^7.0.1",
		"postcss-cli": "^6.1.1",
		"postcss-conditionals": "^2.1.0",
		"postcss-each": "^0.10.0",
		"postcss-for": "^2.1.1",
		"postcss-import": "^12.0.1",
		"postcss-mesh": "^1.1.0",
		"postcss-nested": "^4.1.1",
		"postcss-nested-ancestors": "^2.0.0",
		"postcss-pxtorem": "^4.0.1",
		"postcss-scss": "^2.0.0",
		"postcss-selector-not": "^4.0.0",
		"postcss-will-change": "^3.0.0",
		"style-loader": "^0.23.1",
		"webpack": "^4.29.3",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.14"
	},
	"dependencies": {
		"eases": "^1.0.8"
	}
}
