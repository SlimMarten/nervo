@font-face {
  font-family:"Roboto Black Italic";
  src:resolve("Roboto-BlackItalic.eot?") format("eot"),
  resolve("Roboto-BlackItalic.woff") format("woff"),
  resolve("Roboto-BlackItalic.ttf") format("truetype"),
  resolve("Roboto-BlackItalic.svg#Roboto-BlackItalic") format("svg");
  font-weight:normal;
  font-style:normal;
}

@font-face {
  font-family: 'Roboto Slab Regular';
  src: resolve('RobotoSlab-Regular.woff2') format('woff2'),
        resolve('RobotoSlab-Regular.woff') format('woff');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'Roboto Slab Bold';
  src: resolve('RobotoSlab-Bold.woff2') format('woff2'),
        resolve('RobotoSlab-Bold.woff') format('woff');
  font-weight: normal;
  font-style: normal;
}

